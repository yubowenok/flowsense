# Auto Layout
(def @auto_layout (string "auto-layout"))
(rule $AutoLayoutPhrase (($Verb optional) ($StopPhrase optional) layout) (IdentityFn))
(rule $AutoLayoutPhrase (($Adj optional) layout) (IdentityFn))
(rule $AutoLayoutPhrase (autolayout) (IdentityFn))

(rule $AutoLayout ($AutoLayoutPhrase) (ConstantFn @auto_layout))

### Application ###
(rule $Command ($AutoLayout) (IdentityFn))
